<script>
    $(document).ready(function () {
        $.get("/hrms/students/attendance_status/24011.js", function () {
            $("#attendance_loader").hide();
            $('.progress .progress-bar').progressbar();
        })

          $.get("/cems/events/user_dashboard_events.js", function () {
              $("#event_loader").hide();
          });


        function toggleCGPAVisibility() {
          var eyeIcon = $('#toggle-cgpa i');
          var cgpaVisibility = localStorage.getItem('cgpaVisibility');

          if (cgpaVisibility !== 'hidden') {
            eyeIcon.removeClass('fa-eye-slash').addClass('fa-eye');
            $('#cgpa-number').text('8.0')
            // If minor or honor CGPA info exists, set its value to actual or "N/A"
            $('#minor-cgpa').text('N/A');
            $('#honor-cgpa').text('N/A');
            eyeIcon.attr('title', 'Hide CGPA');
          } else {
            eyeIcon.removeClass('fa-eye').addClass('fa-eye-slash');
            $('#cgpa-number').text('- -')
            // If minor or honor CGPA info exists, toggle its visibility
            $('#minor-cgpa').text('- -');
            $('#honor-cgpa').text('- -');
            eyeIcon.attr('title', 'Reveal CGPA');   
          }
        }

        $('#toggle-cgpa').click(function (event) {
            event.stopPropagation(); 
            var cgpaVisibility = localStorage.getItem('cgpaVisibility'); // Retrieve from local storage

            // Toggle CGPA visibility
            if (cgpaVisibility === 'hidden') {
                localStorage.setItem('cgpaVisibility', 'visible'); // Store in local storage
            } else {
                localStorage.setItem('cgpaVisibility', 'hidden'); // Store in local storage
            }
            toggleCGPAVisibility(); 
        });

        toggleCGPAVisibility();

       $('.user-dashboard-cgpainfo-card').click(function () {
           $(this).toggleClass('d-none d-flex');
       });
    })
</script>



