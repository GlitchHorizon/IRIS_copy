<script>
    $(document).ready(function () {
        $("#scrollDown").on("click" ,function(){
            $("#event_accordion .active .scrollbar-container .search-list-group").animate({
                scrollTop: "+=" + 300 + "px"
            });

        });

        $("#scrollUp").on("click" ,function(){
            $("#event_accordion .active .scrollbar-container .search-list-group").animate({
                scrollTop: "-=" + 300 + "px"
            });
        });

        $("#cems_widget_manage_subscriptions_link").tooltip();
    });
    let feed = 0;
    document.getElementById("my_events").addEventListener('click',function(){
      if(feed === 1){
        document.getElementById("glo_feed_next_week").click();
        document.getElementById("sub_feed_curr_week").click();
        feed = 0;
      }
    });
    document.getElementById("all_events").addEventListener('click',function(){
      if(feed === 0){
        document.getElementById("sub_feed_next_week").click();
        document.getElementById("glo_feed_curr_week").click();
        feed = 1;
      }
    });
</script>
