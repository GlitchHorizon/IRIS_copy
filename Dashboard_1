<script>
    $('#loader').appendTo('body');
    $('#loader-img').appendTo('body');

    function loaderStart() {
        $("#loader").show();
        $("#loader-img").show()
    }

    function loaderStop() {
        $("#loader").hide();
        $("#loader-img").hide();
    }

    $(document).ready(function () {
        $(document).ajaxStart(function () {
            loaderStart()
        })
            .ajaxStop(function () {
                loaderStop()
            });
    });
</script>


